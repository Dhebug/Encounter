'
'
'      D‚coupage des blocs pour la carte
'
'  ½ comme d'habitude...
'
~XBIOS(5,L:-1,L:-1,W:0)
OPEN "i",#1,"block_00.neo"
c$=SPACE$(32)
SEEK #1,4
BGET #1,V:c$,32
~XBIOS(6,L:V:c$)
SEEK #1,128
BGET #1,XBIOS(2),32000
CLOSE
'
DIM a$(11)
FOR i=0 TO 11
  GET 2+i*19,2,2+i*19+15,2+15,a$(i)
  PBOX 2+i*19,2,2+i*19+15,2+15
NEXT i
'
OPEN "o",#1,"decor0.blk"
CLS
FOR i=0 TO 11
  PUT 0,0,a$(i)
  FOR y=0 TO 15
    BPUT #1,XBIOS(2)+y*160,8*2
  NEXT y
NEXT i
CLOSE
